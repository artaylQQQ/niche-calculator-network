    "slug": "percentage-increase",
    "title": "Percentage Increase",
    "cluster": "math",
    "description": "Get the percentage increase from an old value to a new value.",
    "inputs": [
      {
        "name": "oldValue",
        "label": "Old value",
        "type": "number",
        "min": 0,
        "step": "any",
        "placeholder": "50"
      },
      {
        "name": "newValue",
        "label": "New value",
        "type": "number",
        "min": 0,
        "step": "any",
        "placeholder": "70"
      }
    ],
    "expression": "((newValue - oldValue) / oldValue) * 100",
    "examples": [
      {
        "description": "From 50 to 70 ⇒ 40% increase"
      }
    ],
    "faqs": [
      {
